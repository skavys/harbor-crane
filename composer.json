{
    "name": "skavys/harbor-crane",
    "type": "project",
    "description": "Porto Generator scripts",
    "keywords": ["porto", "generator", "scripts", "skavys", "skavy≈°", "cli"],
    "homepage": "https://github.com/skavys/harbor-crane",
    "license": "MIT",
    "authors": [
        {
            "name": "Dzianis Kotau",
            "email": "me@dzianiskotau.com",
            "homepage": "https://dzianiskotau.com/",
            "role": "Owner"
        }
    ],
    "support": {
        "issues": "https://github.com/skavys/harbor-crane/issues",
        "source": "https://github.com/skavys/harbor-crane"
    },
    "require": {
        "php": "^8.1",
        "laravel-zero/framework": "^9.0",
        "nunomaduro/termwind": "^1.3"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/larastan": "^2.1",
        "pestphp/pest": "^1.21.1",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["harbor-crane"]
}
